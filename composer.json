{
    "name": "liip/url-auto-converter-bundle",
    "type": "symfony-bundle",
    "description": "This bundle adds a Twig Extension with a filter for automatically converting urls and emails in a string to html links",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Liip AG",
            "homepage": "http://www.liip.ch/"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/liip/LiipUrlAutoConverterBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.0",
        "twig/twig": "^1.41|^2.10|^3.0",
        "symfony/dependency-injection": "^5.0@dev",
        "symfony/framework-bundle": "^5.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Linnit\\Twig\\Extension\\": "./src/Linnit/Twig/Extension/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Linnit\\Tests\\": "tests/",
            "Linnit\\DependencyInjection\\": "./DependencyInjection/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}
